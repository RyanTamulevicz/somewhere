{
  "name": "address-input",
  "version": "1.0.0",
  "description": "International address input web component using Google's address metadata",
  "type": "module",
  "scripts": {
    "build": "npm run build:cdn",
    "build:cdn": "vite build --config vite.cdn.config.js",
    "dev": "vite --config vite.config.js",
    "fetch-data": "node scripts/fetch-address-data.js",
    "prepare": "npm run fetch-data && npm run build"
  },
  "dependencies": {
    "lit": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "concurrently": "^9.2.1",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vite": "^7.3.1"
  },
  "keywords": [
    "web-components",
    "address",
    "international",
    "lit",
    "input"
  ],
  "license": "MIT"
}